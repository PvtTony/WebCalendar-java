<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.calendar.model.User" table="cal_user">
        <meta attribute="class-description">
            This class contains the user detail.
        </meta>
        <id name="usrID" type="int" column="usr_id">
            <generator class="native"/>
        </id>
        <property name="usrPassword" column="usr_pass" type="java.lang.String"/>
        <property name="usrNickname" column="usr_nickname" type="java.lang.String"/>
        <property name="usrEmail" column="usr_email" type="java.lang.String"/>
    </class>
</hibernate-mapping>